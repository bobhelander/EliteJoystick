<UserControl x:Class="EliteJoystick.ViewControls.LaunchAppControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EliteJoystick.ViewControls"
             mc:Ignorable="d">
    <Grid>
        <CheckBox Content="{Binding Path=Name}" Tag="{Binding Path=Command}" IsChecked="{Binding Path=Launch}"/>        
    </Grid>
</UserControl>
